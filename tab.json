{
  "title": "Tab to right_command upon hold, tab upon tap",
  "rules": [
    {
    "description": "Makes tab behave like left_control upon holding, tab upon tap.",
    "manipulators": [
      {
      "from": {
        "key_code": "tab",
        "modifiers": {
          "optional": [
            "any"
          ]
        }
      },
      "to_if_alone": [
        {
        "key_code": "tab"
      }
      ],
      "to": [
        {
        "key_code": "left_control"
      }
      ],
      "type": "basic"
    }
    ]
  }
  ]
}
